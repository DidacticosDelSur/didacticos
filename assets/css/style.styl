@import url('https://fonts.googleapis.com/css?family=Montserrat:300,400,500,500i,600,700,800&display=swap');


// var

$primary-color = #3390ff
$second-color = #ff3d57


body
    font-family 'Montserrat'
    padding 0
    margin 0
    font-size 16px
    color #313333
    overflow-x hidden
    background #f5f5f5


h1, h2, h3, h4, h5, p, a, input, figure, figcaption, select, button, textarea
    -webkit-appearance none
    margin 0
    display block
    outline none
    color #454545
    font-family 'Montserrat'
    border none
    padding 0

a
    text-decoration none
    font-size 12px
    font-weight 500


button, a
    cursor pointer
    background none
    
.wide-content
    max-width 1122px
    margin 0 auto
    width 96%

.flexBlock
    display flex
    align-items center
    justify-content space-between

header
    height auto
    background white
    border-bottom 1px solid #bbbbbb
    padding 20px 0 15px
    .wide-content
        .flexBlock
            &:first-of-type
                margin-bottom 10px
                .group
                    justify-content flex-start
                    width 100%
                    max-width 780px
                    @media(max-width:1121px)
                        max-width inherit
                        width 88%
                    h1
                        width 163px
                        margin-right 25px
                        @media(max-width:1121px)
                            width 106px
                        a
                            width 100%
                            img
                                width 100%
                    .fieldSearch
                        width 100%
                        justify-content flex-start
                        border 1px solid #d3d3d3
                        border-radius 8px
                        -moz-border-radius 8px
                        -webkit-border-radius 8px
                        padding 5px 0 5px 10px
                        margin 0
                        @media(max-width:1121px)
                            display none
                        input, button
                            height 25px
                        input
                            width calc(100% - 50px)
                        button
                            width 48px
                            border-left 1px solid #d3d3d3
                            display flex
                            align-items center
                            justify-content center
                            img
                                width 19px
                                cover-fit contain
                .register
                    border 1px solid #c8c8c8
                    padding 10px 25px
                    @media(max-width:1121px)
                        display none
                .mobile-items
                    display none
                    @media(max-width:1121px)
                        display flex
            &:last-of-type
                nav
                    padding-left 154px
                    margin 0
                    a
                        margin-right 15px
                        color #454545
                        position relative
                        &:last-of-type
                            margin 0
                            @media(max-width:1121px)
                                padding-top 25px
                            label
                                position absolute
                                top -16px
                                left 50%
                                transform translateX(-50%)
                                padding 2px 6px
                                text-transform uppercase
                                color white
                                background $second-color
                                border-radius 6px
                                -moz-border-radius 6px
                                -webkit-border-radius 6px
                                font-size 9px
                                font-weight 600
                                @media(max-width:1121px)
                                    top 4px
                                    left 16px
                                    transform none
                                    
                .menu-cart
                    margin 0
                    @media(max-width:1122px)
                        display none
                    a
                        padding 6px 15px
                        margin-right 15px
                        border-right 1px solid #aaa
                        border-left 1px solid #aaa
                    div
                        display flex
                        align-items center
                        justify-content flex-start
                        img
                            width 28px
                            margin -7px 0 0 0
                        span
                            font-weight 500
                            font-size 12px
                            margin 0 0 0 7px
                            color #454545
                            &.added
                                color $second-color
                .itemsMobile
                    display none
                    @media(max-width:1121px)
                        display block
                        div
                            display block
                            border-top 1px solid #ababab
                            padding 35px 0 35px 20px
                            span, a
                                display block
                            a
                                font-size 16px
                                color #454545
                                font-weight 500
                                display flex
                                justify-content flex-start
                                img
                                    margin-right 8px 
                            &:first-of-type
                                a
                                    img
                                        width 33px                                  
                            &:nth-of-type(2)
                                a
                                    color $primary-color
                            &:last-of-type
                                a
                                    img
                                        width 34px 
                    
            &.hideMobile
                @media(max-width:1121px)
                    display none
                    position absolute
                    top 97px
                    background white
                    padding 15px 0
                    width 100%
                    left 0
                    z-index 99999
                    nav
                        display block
                        padding 0 0 25px
                        a
                            display block
                            padding 25px 0 25px 20px
                            text-align left
                            font-size 16px
                            margin 0
                            &:hover
                                color $primary-color
                                background #f5f5f5

.stage
    margin-bottom 45px

.categories
    margin-bottom 55px
    @media(max-width:1000px)
        flex-direction column
    article
        width 32%
        height 446px
        position relative
        @media(max-width:1000px)
            width 100%
            margin-bottom 20px
        .frame
            position absolute
            right 10%
            top 5%
            width calc(168px - 40px)
            height calc(168px - 40px)
            padding 20px
            &.blue-color
                background $primary-color
            &.red-color
                background $second-color
            h2, .title
                position absolute
                color white
            h2
                font-size 9px
                text-align left
                font-weight 300
                text-transform uppercase
                display block
                top 15px
                right 15px
            .title
                font-weight 800
                font-size 36px
                line-height 36px
                text-align right
                bottom 20px
                right 20px
        img
            width 100%
            height 100%
            object-fit cover
        &:last-of-type
            .box
                height 50%
                display flex
                align-items center
                justify-content center
                .title
                    margin-left -45%
                    color white
                    font-size 22px
                    font-weight 800
                    a
                        margin 20px 0 0 0
                        display block
                        padding 8px
                        text-align center
                        border 1px solid white
                        color white
                        width 100px
                &:first-of-type
                    background #ecba3c url(../images/littleGirl.jpg) center right no-repeat
                    background-size contain
                &:last-of-type
                    background #efefef url(../images/agenda.png) top right no-repeat
                    background-size 50%
                    @media(max-width:1000px)
                        background #efefef url(../images/agenda.png) top right 10% no-repeat
                        background-size 20%
                    .title
                        color #3a3a3a
                        a
                            color #3a3a3a
                            border-color #3a3a3a

.moreBuy
    margin 25px 0 0 0

.doubleProducts
    margin 20px 0 40px
    border-top 1px solid #d0d0d0
    padding-top 10px
    @media(max-width:1000px)
        flex-direction column
    .containerSlider
        width 50%
        @media(max-width:1000px)
            width 100%
        .flexBlock
            padding-bottom 15px
            margin-bottom 25px
            justify-content flex-start
            position relative
            &:after
                position absolute
                left 0
                bottom 0
                width calc(100% - 18px)
                background #d0d0d0
                height 1px
                content ''
            h4
                margin 0
                padding 0
                border 0
            &.title
                img
                    width 19px
                    margin-top -3px
                h4
                    margin 0 60px 0 15px
                a
                    color $primary-color
                    font-size 12px
        &:last-of-type
            @media(max-width:1000px)
                margin-top 20px
                border-top 1px solid #d0d0d0
                padding-top 15px
            .flexBlock
                padding-left 22px
                @media(max-width:1000px)
                    padding 0 0 15px
                &:after
                    left 22px
                    width calc(100% - 21px)
                    @media(max-width:1000px)
                        left 0

.productsItems
    position relative
    .slick-arrow
        width 71px
        height 68px
        position absolute
        top 50%
        transform translateY(-50%)
        z-index 999
        display none !important
        &.slick-prev
            left -17px
            background url(../images/left-arrow.png) 0 0 no-repeat
            background-size contain
        &.slick-next
            right -17px
            background url(../images/right-arrow.png) 0 0 no-repeat
            background-size contain
    &:hover
        .slick-arrow
            display block !important
    .slick-slide
        padding 0 20px
        height 394px
        .frame
            height 390px
            background white
            border 1px solid #e5e5e5
            position relative
            .discount
                position absolute
                top 4%
                left 7%
                background #16b36e
                padding 4px 6px
                font-weight 800
                font-size 12px
                color white
            .image
                height 274px
                border-bottom 1px solid #e5e5e5
                justify-content center
                padding 0
                margin 0
                &:after
                    display none
                img
                    width 80%
                    object-fit contain
                    height 50%
            .description
                height calc(115px - 40px)
                padding 20px
                .categ
                    font-size 12px
                    font-weight 500
                    font-style italic
                    color #6a6a6a
                .name, .price
                    font-weight 800
                .name
                    color #404040
                    font-size 14px
                    margin 5px 0 15px 0
                .price
                    color $primary-color
                    font-size 16px
                    span
                        display inline-block
                        margin-left 6px
                        font-weight 400
                        text-decoration line-through
                        color #aeaeae
                        font-size 12px
