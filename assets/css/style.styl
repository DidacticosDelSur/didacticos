.categoriesSect
    margin 40px 0 120px
    .breadcrumb
        display block
        margin-bottom 15px
        a, span
            display inline
            font-size 12px
            font-weight 400
    .flexBlock
        align-items flex-start
        @media(max-width:960px)
            flex-direction column
        aside
            width 150px
            padding-top 10px
            @media(max-width:960px)
                margin-bottom 20px
                width 100%
            h2
                color $primary-color
                font-size 24px
                font-weight 600
                margin-bottom 30px
                small
                    display block
                    margin-top 10px
                    font-size 12px
                    font-weight 400
                    color #454545
            h3, button
                display block
            h3
                margin 25px 0 10px
                font-size 16
                font-weight 600
                &:first-of-type
                    margin-top 0
            button
                margin-bottom 5px
                &:hover
                    color $primary-color
        section
            width calc(100% - 170px)
            flex-wrap wrap
            @media(max-width:960px)
                width 100%
            .frame
                width 24.5%
                margin-bottom 10px
                @media(max-width:1200px)
                    width 32.5%
                @media(max-width:960px)
                    width 49.5%
                @media(max-width:640px)
                    width 100%
                .flexBlock
                    align-items center
            
.sponsor
    .frame
        display flex !important
        justify-content center
        height 60px
        align-items center    
        div
            text-align center
            img
                display inline-block

footer
    background white
    margin-top 60px
    border-top 1px solid #e5e5e5
    .flexBlock
        @media(max-width:1000px)
            flex-direction column
        &:first-of-type
            padding 40px 0 60px
            .module
                text-align center
                @media(max-width:1000px)
                    margin-bottom 40px
                    &:last-of-type
                        margin 0
                img
                    margin 0 auto 25px
                    object-fit contain
                &:first-of-type, &:last-of-type
                    img
                        width 51px
                &:nth-of-type(2)
                    img
                        width 65px
                h5, p
                    font-weight 400
                p, a
                    font-size 12px
                h5
                    color #454545
                    font-size 18px
                p
                    font-size 12px
                    margin 8px auto 25px
                    color #979797
                a
                    color $primary-color
        &:nth-of-type(2)
            border-bottom 1px solid #d0d0d0
            border-top 1px solid #d0d0d0
            padding 36px 0
            .logo
                width 163px
                @media(max-width:1000px)
                    margin-bottom 30px
                img
                    width 100%
            .item
                @media(max-width:1000px)
                    text-align center
                    width 100%
                    margin-bottom 30px
                a
                    color #454545
                    font-size 12px
                    @media(max-width:1000px)
                        margin-bottom 15px
                        &:last-of-type
                            margin 0
            .register
                padding 10px 25px
                background black
                color white
        &:last-of-type
            padding 10px 0 25px
            @media(max-width:1000px)
                padding-top 20px
            p
                font-size 12px
                @media(max-width:1000px)
                    margin-bottom 10px
                    &:last-of-type
                        margin 0
                a
                    display inline-block
                    font-weight 800


h4
    color #454545
    font-size 18px
    display block
    text-align center
    padding-bottom 10px
    border-bottom 1px solid #d0d0d0
    font-weight 500
    &.text-left
        text-align left
        margin-bottom 40px

.slick-dots
    display none !important

.shop-wrap
    padding 40px 0 120px
    background #f5f5f5
    .container-shop
        position relative
        background white
        border 1px solid #e5e5e5
        padding 25px 40px 35px
        .number-items
            padding-bottom 15px
            border-bottom 1px solid #d0d0d0
            font-size 14px
            color #464646
            font-weight 600
            text-indent 5px
        .item
            padding 30px 0
            border-bottom 1px solid #d0d0d0
            @media(max-width:900px)
                flex-direction column
                align-items flex-start
            img
                width 125px
                object-fit contain
                @media(max-width:900px)
                    margin 0
            .detail
                margin-left -13%
                @media(max-width:1180px)
                    margin 0
                @media(max-width:900px)
                    width 100%
                    margin 15px 0 25px
                .name, .categ
                    color #464646
                .name
                    font-size 18px
                    font-weight 400
                .categ
                    font-size 12px
                    font-weight 300
                    margin 6px 0 30px
                button
                    font-size 12
                    font-weight 500
                    color $primary-color
            .number-mod
                width calc(100px - 30px)
                border 1px solid #d0d0d0
                border-radius 8px
                -moz-border-radius 8px
                -webkit-border-radius 8px
                height 40px
                margin-top -40px
                padding 0 15px
                button, span
                    font-weight 500
                    color #464646
                    font-size 16px
                button
                    font-size 21px
                    color #a5a5a5
                    &:last-of-type
                        color $primary-color
            .quantBox
                .quantity
                    display block
                    margin 0 auto 20px
                    padding 10px 30px
                    border-radius 15px
                    border 1px solid #d0d0d0
                    position relative
                    width calc(100px - 60px)
                    .input
                        text-align center
                        input
                            width auto
                            -moz-appearance textfield
                            text-align center
                            width 100%
                            font-size 16px
                            font-family 'Open Sans'
                            color #464646
                    .quantity-button
                        position absolute
                        top 50%
                        transform translateY(-50%)
                        font-size 22px
                        font-family 'Montserrat'
                        font-weight 500
                        cursor pointer
                        &.quantity-down
                            left 15px
                            top 48%
                            color #a5a5a5
                        &.quantity-up
                            right 15px
                            color $primary-color
                .value
                    color #464646
                    font-size 14px
                    font-family 'Open Sans'
                    strong
                        display inline
                        color #091129
                        font-weight 600
            .pricing
                margin-top -20px
                @media(max-width:900px)
                    margin 0
                .discount
                    font-size 14px
                    font-weight 500
                    color $second-color
                    text-transform uppercase
                    span
                        display inline-block
                        vertical-align middle
                        margin-left 10px
                        text-decoration line-through
                        color #8f8f8f
                        font-weight 300
                .priceUnit
                    font-size 30px
                    color #464646
                    font-weight 500
                    text-align right
                    margin-top 5px
                    @media(max-width:900px)
                        text-align left
        .totalPrice
            padding 40px 0 30px
            border-bottom 1px solid #d0d0d0
            div
                justify-content flex-end
                margin-bottom 15px
                @media(max-width:900px)
                    flex-direction column
                &:first-of-type, &:last-of-type
                    font-size 16px
                    color #464646
                    font-weight 500
                &:nth-of-type(2)
                    color $primary-color
                    font-size 14px
                    font-weight 300
                &:last-of-type
                    margin 0
                    font-size 24px
                span
                    text-align right
                    &:first-of-type
                        width 80%
                    &:last-of-type
                        width 20%
                    @media(max-width:900px)
                        text-align left
                        width 100% !important
        .button
            padding 50px 0 20px
            justify-content flex-end
            @media(max-width:900px)
                justify-content center
            button
                -moz-border-radius 8px
                -webkit-border-radius 8px
                border-radius 8px
                background $primary-color
                color white
                font-size 14px
                font-weight 400
                padding 15px 20px
        &.shopDetail
            padding 0
            display flex
            justify-content space-between
            align-items flex-start
            margin-bottom 55px
            @media(max-width:960px)
                flex-direction column
            .box
                width calc(50% - 140px)
                padding 35px 70px 30px
                height 500px
                @media(max-width:960px)
                    width calc(100% - 140px)
                &:first-of-type
                    border-right 1px solid #e5e5e5
                    @media(max-width:960px)
                        border 0
                        border-bottom  1px solid #e5e5e5
                    .slick-slider, .slick-list, .slick-track, .slick-slide
                        height 100%
                    .slick-slide
                        div
                            display flex
                            justify-content space-between
                            height 100%
                            .flexBlock
                                display flex !important
                                align-items center
                                justify-content center
                                img
                                    width 960%
                                    object-fit contain
                    .slick-dots
                        display flex !important
                        align-items center
                        justify-content center
                        padding 0
                        list-style-type none
                        position absolute
                        bottom -10px
                        left 0
                        width 100%
                        li
                            margin 0 6px
                            button
                                text-indent -10000px
                                border-radius 50%
                                -moz-border-radius 50%
                                -webkit-border-radius 50%
                                background #afafaf
                                height 12px
                                width 12px
                                border 2px solid #afafaf
                            &.slick-active, &:hover
                                button
                                    background white
                &:last-of-type
                    padding 35px 100px
                    width calc(50% - 200px)
                    @media(max-width:1080px)
                        padding 35px 50px
                        width calc(50% - 100px)
                    @media(max-width:960px)
                        width calc(100% - 100px)
                    h2, .price
                        font-size 30px
                        margin 15px 0
                        font-weight 500
                    p
                        font-size 12px
                    .breadcrumb
                        a, span
                            display inline
                            color #464646
                            font-size 12px
                            font-weight 500
                    .size
                        margin 20px 0 20px
                        span
                            display inline-block
                            vertical-align middle
                            margin-right 5px
                            border-radius 50%
                            -moz-border-radius 50%
                            -webkit-border-radius 50%
                            background $primary-color
                            color white
                            text-align center
                            font-size 14px
                            height 27px
                            width 27px
                            line-height 26px
                    .price-box
                        padding 15px 0
                        border-top 1px solid #e5e5e5
                        border-bottom 1px solid #e5e5e5
                        .discount
                            display block
                            margin-bottom 10px
                            color #8f8f8f
                            font-size 16px
                            font-weight 300
                            text-decoration line-through
                        .price
                            line-height 27px
                            margin 10px 0 0
                            span
                                display inline-block
                                margin 0 0 0 5px
                                vertical-align baseline
                                color $second-color
                                font-size 16px
                                text-transform uppercase
                    .cant-box
                        padding 30px 0 50px
                        span, .selectField
                            display inline-block
                            vertical-align middle
                        .selectField
                            position relative
                            cursor pointer
                            &:after
                                content ''
                                position absolute
                                right 6px
                                top 38%
                                transform translateY(-50%) rotate(-45deg)
                                height 6px
                                width 6px
                                border-bottom 2px solid $primary-color
                                border-left 2px solid $primary-color
                                z-index 9
                            select
                                padding-right 25px
                                position relative
                                z-index 99
                        span, select
                            font-size 14px
                            font-weight 500
                            background none
                        span
                            margin-right 20px
                    .buttons
                        button
                            display inline-block
                            vertical-align middle
                            &:first-of-type
                                margin-right 14px
    &.modal-window
        .flexBlock
            justify-content center
            padding 140px 0 50px
            position relative
            &:before
                position absolute
                top 56px
                left 50%
                width 90%
                transform translateX(-50%)
                content ''
                z-index 9
                height 1px
                background #e5e5e5
            .info
                text-align center
                img, h2, p, button
                    display block
                img
                    width 159px
                    object-fit contain
                    margin 0 auto
                h2
                    margin 45px auto 0
                    font-weight 400
                    font-size 18px
                p
                    margin 15px auto 55px
                    font-size 14px
                button
                    margin 0 auto
    &.modal-user
        .container-shop
            max-width calc(580px - 160px)
            margin 0 auto 30px
            padding 60px 80px 80px
            h2, input
                display block
                width 100%
            h2
                font-size 18px
                text-align center
                margin-bottom 75px
            input
                border-bottom 1px solid #e5e5e5
                padding 0 0 5px
                font-size 16px
                color #979797
                margin-bottom 60px
                &:last-of-type
                    margin 0
        .button-row
            text-align center
            button
                margin 0 auto
                width 180px
            .only-text
                margin 20px auto 25px
                width 100%
                color $primary-color
                font-size 14px
                font-weight 600
            p
                font-size 11px
                font-weight 300
                color #797979
                a
                    
                    display inline
                    color $primary-color
        &.forgot-modal
            img
                width 88px
                margin 0 auto
                display block
                object-fit contain
            h2
                margin 25px auto 15px
            p
                text-align center
                font-size 11px
                margin-bottom 60px
            .button-row
                p
                    margin-top 20px
        &.register-modal
            padding-top 130px
            .container-shop
                max-width calc(820px - 160px)
                width calc(100% - 160px)
                @media(max-width:960px)
                    width calc(100% - 120px)
                    padding 60px
                h2
                    position absolute
                    top -50px
                    left 0
                    text-align left
                .flexBlock
                    @media(max-width:960px)
                        flex-direction column
                    div
                        width 44%
                        @media(max-width:960px)
                            width 100%
                            margin 0
                            input
                                margin-bottom 25px
            .button-row
                text-align left
                max-width 820px
                margin 0 auto
